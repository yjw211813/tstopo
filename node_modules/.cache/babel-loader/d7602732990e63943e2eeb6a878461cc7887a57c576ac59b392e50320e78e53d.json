     activeComponent.value = 'NetCalculation';\r\n      }\r\n    };\r\n\r\n    return {\r\n      activeIndex,\r\n      activeName,\r\n      activeComponent,\r\n      handleSelect\r\n    };\r\n  }\r\n}\r\n</script>\r\n\r\n<style>\r\n.network-diagram-container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.network-topology {\r\n  width: 70%;\r\n  border-right: 1px solid #ddd;\r\n}\r\n\r\n.function-menu {\r\n  width: 30%;\r\n  padding: 10px;\r\n}\r\n\r\n.tabs {\r\n  margin-top: 10px;\r\n}\r\n</style>\r\n"],"mappings":"AA2BA,SAAQA,GAAG,QAAO,KAAI;AACtB,SAAQC,MAAM,EAAEC,UAAU,EAAEC,MAAM,EAAEC,SAAS,QAAO,cAAa;;AAEjE;AACA,OAAOC,cAAa,MAAO,qCAAoC;AAC/D,OAAOC,YAAW,MAAO,oBAAmB;AAC5C,OAAOC,iBAAgB,MAAO,yBAAwB;AACtD,OAAOC,eAAc,MAAO,uBAAsB;AAElD,eAAe;EACbC,UAAU,EAAE;IACVR,MAAM;IACNC,UAAU;IACVC,MAAM;IACNC,SAAS;IACTC,cAAc;IACdC,YAAY;IACZC,iBAAiB;IACjBC;EACF,CAAC;EACDE,KAAKA,CAAA,EAAG;IACN,MAAMC,WAAU,GAAIX,GAAG,CAAC,GAAG,CAAC;IAC5B,MAAMY,UAAS,GAAIZ,GAAG,CAAC,MAAM,CAAC;;IAE9B;IACA,MAAMa,eAAc,GAAIb,GAAG,CAAC,gBAAgB,CAAC;IAE7C,MAAMc,YAAW,GAAKC,GAAG,IAAK;MAC5B;MACA,QAAQA,GAAG;QACT,KAAK,GAAG;UACNF,eAAe,CAACG,KAAI,GAAI,gBAAgB;UACxC;QACF,KAAK,GAAG;UACNH,eAAe,CAACG,KAAI,GAAI,cAAc;UACtC;QACF,KAAK,GAAG;UACNH,eAAe,CAACG,KAAI,GAAI,mBAAmB;UAC3C;QACF,KAAK,GAAG;UACNH,eAAe,CAACG,KAAI,GAAI,iBAAiB;UACzC;QACF;UACEH,eAAe,CAACG,KAAI,GAAI,gBAAgB;MAC5C;IACF,CAAC;IAED,OAAO;MACLL,WAAW;MACXC,UAAU;MACVC,eAAe;MACfC;IACF,CAAC;EACH;AACF","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}